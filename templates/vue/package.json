{
  "name": "tapestry-d3-vue",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "Tapestry Team, University of British Coloumbia",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --progress",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build:stats": "webpack --mode production --json > stats.json",
    "lint": "eslint \"src/**/*.{js,vue}\"",
    "lint:fix": "npm run lint -- --fix",
    "test:e2e": "cypress run --headless -b chrome",
    "test:e2e:open": "cypress open",
    "test:jest": "jest templates/vue/src",
    "jest": "jest",
    "test": "npm run test:jest && npm run test:e2e && npm run report:merge",
    "prereport:merge": "mv coverage-jest/coverage-final.json .nyc_output",
    "report:merge": "nyc merge .nyc_output coverage/coverage.json",
    "report:jest": "open coverage-jest/lcov-report/index.html",
    "report:open": "nyc report --reporter=html && open coverage/index.html",
    "report": "nyc report --reporter=text-summary",
    "start": "npm run dev"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "audio-recorder-polyfill": "^0.3.4",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.13.1",
    "d3": "^5.16.0",
    "dragselect": "^1.13.1",
    "leaflet": "^1.7.1",
    "match-sorter": "^5.0.0",
    "moment": "^2.25.3",
    "moment-timezone": "^0.5.28",
    "tiptap": "^1.29.1",
    "tiptap-extensions": "1.31.1",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue-select": "^3.10.8",
    "vue-slicksort": "^1.1.3",
    "vue-youtube-embed": "^2.2.2",
    "vue2-leaflet": "^2.5.2",
    "vuex": "^3.1.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@cypress/code-coverage": "^3.8.1",
    "@testing-library/cypress": "^7.0.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/user-event": "^12.2.0",
    "@testing-library/vue": "^5.0.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-istanbul": "^6.0.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "cypress": "5.3.0",
    "cypress-file-upload": "^4.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^1.1.4",
    "jest": "^26.4.2",
    "node-sass": "^4.13.0",
    "prettier": "^1.18.2",
    "sass-loader": "7.3.1",
    "style-loader": "^1.0.0",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.3"
  },
  "nyc": {
    "extension": [
      ".js",
      ".vue"
    ],
    "reporter": [
      "html"
    ]
  }
}
